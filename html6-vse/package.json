{
  "name": "html6",
  "displayName": "HTML6",
  "description": "HTML 6 and CSS 4 VSCode Extension - Syntax Highlight",
  "version": "0.0.1",
  "icon": "./html6.png",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "htmlsx",
      "aliases": ["HTML6 / CSS4 Languages", "htmlsx"],
      "extensions": [".html6", ".css4"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "htmlsx",
      "scopeName": "source.htmlsx",
      "path": "./syntaxes/htmlsx.tmLanguage.json"
    }]
  }
}
